<p id="notice"><%= notice %></p>

<h1>Rescates</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Childs</th>
      <th>Adults</th>
      <th>Note</th>
      <th>Phone</th>
      <th>Status</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rescates.each do |rescate| %>
      <tr>
        <td><%= rescate.firstname %></td>
        <td><%= rescate.lastname %></td>
        <td><%= rescate.childs %></td>
        <td><%= rescate.adults %></td>
        <td><%= rescate.note %></td>
        <td><%= rescate.phone %></td>
        <td><%= rescate.status %></td>
        <td><%= rescate.latitude %></td>
        <td><%= rescate.longitude %></td>
        <td><%= link_to 'Show', rescate %></td>
        <td><%= link_to 'Edit', edit_rescate_path(rescate) %></td>
        <td><%= link_to 'Destroy', rescate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rescate', new_rescate_path %>
